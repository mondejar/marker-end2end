layer {
  name: "label"
  type: "Data"
  top: "label"
  top: "label2"
  include {
    phase: TRAIN
  }
  data_param {
    source: "/home/mondejar/markers_end2end/LMDB/128/training/markers_labels_LMDB"
    batch_size: 64
    backend: LMDB
  }
